{
  "name": "lib",
  "version": "2.0.0",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "clean": "rm -Rf ./dist",
    "up": "npm-check-updates -u && npm install && npm dedupe && npm audit fix",
    "test": "jest"
  },
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "exports": {
    ".": "./dist/lib.js",
    "./asset": "./dist/asset.js",
    "./fml": "./dist/fml.js",
    "./media": "./dist/media.js",
    "./gear": "./dist/gear.js"
  },
  "typesVersions": {
    "*": {
      "asset": [
        "dist/asset"
      ],
      "fml": [
        "dist/fml"
      ],
      "media": [
        "dist/media"
      ],
      "gear": [
        "dist/gear"
      ]
    }
  },
  "author": "Kaede Fujisaki",
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "jest": "^27.0.4",
    "npm-check-updates": "^11.7.1",
    "ts-jest": "^27.0.3"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.5.0",
    "typescript": "^4.3.4"
  }
