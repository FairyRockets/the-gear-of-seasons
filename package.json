{
  "name": "the-gear-of-seasons",
  "version": "1.0.0",
  "description": "the heart of hexe.net",
  "scripts": {
    "build": "gulp build",
    "check": "go generate github.com/fairy-rockets/the-gear-of-seasons/cmds/sanity-check && go build -o .bin/sanity-check github.com/fairy-rockets/the-gear-of-seasons/cmds/sanity-check && .bin/sanity-check",
    "gc": "go generate github.com/fairy-rockets/the-gear-of-seasons/cmds/garbage-collect && go build -o .bin/garbage-collect github.com/fairy-rockets/the-gear-of-seasons/cmds/garbage-collect && .bin/garbage-collect",
    "watch": "gulp watch",
    "deploy": "gulp deploy",
    "backup": "rsync --delete -auzv -e ssh nodoca:/opt/www/fairy-rockets/_shelf/ _shelf/",
    "update": "npm-check-updates -u -a"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fairy-rockets/the-gear-of-seasons.git"
