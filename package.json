{
  "name": "the-gear-of-seasons",
  "version": "1.0.0",
  "description": "the heart of hexe.net",
  "scripts": {
    "start": "gulp watch",
    "deploy": "gulp deploy",
    "build": "mkdir -p .bin/ && go generate ./... && go build -o .bin/ ./... && gulp 'client:build'",
    "clean": "rm -Rf .bin/",
    "check": "npm run build && .bin/sanity-check",
    "gc": "npm run build && .bin/garbage-collect",
    "pull": "rsync --delete -auzv -e ssh hexe.net:/opt/www/hexe.net/the-gear-of-seasons/_shelf/ _shelf/",
    "push": "rsync --delete -auzv -e ssh _shelf/ hexe.net:/opt/www/hexe.net/the-gear-of-seasons/_shelf/",
    "update": "npm-check-updates -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fairy-rockets/the-gear-of-seasons.git"
  },
  "author": "PSI",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/fairy-rockets/the-gear-of-seasons/issues"
  },
  "homepage": "https://github.com/fairy-rockets/the-gear-of-seasons#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "ansi-colors": "^4.1.1",
    "babel-loader": "^8.0.6",
    "child_process": "^1.0.2",
    "del": "^5.1.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "npm-check-updates": "^3.2.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-stream": "^5.2.1",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "gl-matrix": "^3.1.0",
    "twemoji": "^12.1.4"
  }
}
